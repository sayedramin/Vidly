@model Vidly.Models.Movies

@{
    ViewBag.Title = "Save";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre</li>
    <li>Release Date: @Model.ReleaseDate.ToLongDateString()</li>
    <li>Added Date: @Model.AddedDate.ToLongDateString()</li>
    <li>Number In Stock: @Model.NumberInStock</li>
</ul>

